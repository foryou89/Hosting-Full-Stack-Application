{
  "name": "udagram-api",
  "version": "2.0.0",
  "description": "",
  "engines": {
    "node": "16.18.0"
  },
  "main": "server.ts",
  "scripts": {
    "start": "node ./server.js",
    "tsc": "npx tsc",
    "dev": "npx ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "prod": "npx tsc && node ./www/server.js",
    "clean": "rm -rf www/ || true",
    "deploy": "npm install && npm run build && eb init udagram-app --platform node.js --region us-east-1 && eb use Udagramapp-env && eb setenv PORT=$PORT POSTGRES_USERNAME=$POSTGRES_USERNAME POSTGRES_PASSWORD=$POSTGRES_PASSWORD POSTGRES_DB=$POSTGRES_DB POSTGRES_HOST=$POSTGRES_HOST JWT_SECRET=$JWT_SECRET && eb deploy",
    "build": "npm run clean && tsc && cp -rf src/config www/config && cp .npmrc www/.npmrc && cp package.json www/package.json && cd www && zip -r Archive.zip . && cd ..",
    "test": "echo Error: no test specified"
  },
  "keywords": [],
  "author": "Gabriel Ruttner",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/jsonwebtoken": "^8.5.9",
    "aws-sdk": "^2.1241.0",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.5.0",
    "sequelize-typescript": "^2.1.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.37",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "^11.15.54",
    "@types/sequelize": "^4.28.14",
    "@types/validator": "^13.7.9",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^6.2.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.2.3"
  }
